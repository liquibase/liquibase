<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <property name="foo" value="bar"/>
    <include file="include-container.xml" relativeToChangelogFile="true" logicalFilePath="include-container">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="NOT_EXISTENT_TABLE" />
        </preConditions>
    </include>

    <include file="included.xml" relativeToChangelogFile="true" logicalFilePath="included">
        <preConditions onFail="HALT">
            <changeLogPropertyDefined property="foo" value="bar"/>
        </preConditions>
    </include>

    <changeSet id="default" author="cagliostro">
        <createTable tableName="DEFAULT_TABLE">
            <column name="ID" type="BIGINT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>