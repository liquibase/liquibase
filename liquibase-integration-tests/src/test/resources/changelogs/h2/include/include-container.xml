<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <property name="bar" value="foo"/>
    <include file="included1.xml" relativeToChangelogFile="true" logicalFilePath="included1">
        <preConditions onFail="CONTINUE">
            <changeLogPropertyDefined property="bar" value="foo"/>
        </preConditions>
    </include>

    <changeSet id="include-container"  author="cagliostro">
        <createTable tableName="INCLUDE_CONTAINER_TABLE">
            <column name="ID" type="BIGINT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
